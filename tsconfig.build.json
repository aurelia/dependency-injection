{
  "compilerOptions": {
    "module": "amd",
    "moduleResolution": "node",
    "target": "es5",
    "lib": [
      "es2017",
      "dom"
    ],
    "outFile": "dist/doc-temp/aurelia-dependency-injection.js",
    "declaration": false,
    "noImplicitAny": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": false,
    "esModuleInterop": true,
    "stripInternal": true,
    "removeComments": false,
    "skipLibCheck": true,
    "strict": true
  },
  "include": [
    "dist"
  ],
  "exclude": [
    ".vscode",
    "doc",
    "node_modules"
  ]
}
